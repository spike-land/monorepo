{
  "name": "@zedvision/ipfs",
  "version": "11.10.7",
  "sideEffects": false,
  "files": [
    "*/*"
  ],
  "keywords": [
    "ipfs"
  ],
  "authors": [
    "Zoltan Erdos <zolika84@gmail.com>"
  ],
  "scripts": {
    "clean": "rm -rf dist && yarn types",
    "types": "yarn tsc -d",
    "build:client": "yarn esbuild  src/ipfs.client.ts --bundle --platform=browser --minify --format=esm  --tree-shaking=ignore-annotations  --outfile=dist/ipfs.client.js",
    "build:server": "yarn esbuild  src/ipfs.server.ts --bundle --platform=browser --minify --format=iife  --tree-shaking=ignore-annotations --outfile=dist/ipfs.server.js",
    "build": "yarn build:client && yarn build:server"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zed-vision/monorepo"
  },
  "homepage": "https://github.com/zed-vision/monorepo",
  "repository": "https://github.com/zed-vision/monorepo",
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "4ce53f6c1ec82734658b26195f6410696b8d2d9b",
  "dependencies": {
    "@zedvision/ipfs": "^11.10.7",
    "cids": "1.1.5",
    "esbuild": "0.8.33",
    "ipfs": "0.53.0",
    "ipfs-message-port-client": "0.4.4",
    "ipfs-message-port-server": "0.5.0",
    "typescript": "4.2.0-beta"
  }
}
