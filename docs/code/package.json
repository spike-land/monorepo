{
    "name": "@zedvision/code",
    "version": "8.1.1",
    "description": "zed.vison/code",
    "main": "dist/codeLoader.min.js",
    "files": [
        "**/*"
    ],
    "scripts": {
        "clean": "rm -rf dist && mkdir dist",
        "build:sw": "deno bundle  --unstable --import-map ../../import_map.json --no-check src/sw.ts sw.js",
        "build": "yarn clean && deno fmt src && deno bundle --unstable --import-map ../../import_map.json --no-check src/codeBox.ts dist/codeLoader.js && yarn build:sw && yarn minify",
        "minify": "yarn esbuild dist/codeLoader.js --minify --outfile=dist/codeLoader.min.js"
    },
    "keywords": [
        "monaco",
        "editor",
        "react",
        "typescript",
        "html",
        "vscode"
    ],
    "authors": [
        "Zoltan Erdos <zolika84@gmail.com>"
    ],
    "bugs": {
        "url": "https://github.com/zed-vision/monorepo"
    },
    "homepage": "https://code.zed.vision",
    "repository": "https://github.com/zed-vision/monorepo",
    "devDependencies": {
        "esbuild": "0.8.21"
    },
    "gitHead": "db133fb2edf1a37f0e340e26d8aa28c8763b93c2",
    "author": "Zoltan Erdos <zolika84@gmail.com>",
