{
    "name": "@zedvision/diff",
    "version": "10.14.1",
    "description": "Diffs 2 strings",
    "exports": "./src/diff.js",
    "private": false,
    "sideEffects": false,
    "scripts": {
        "npm:install": "rm -rf package-lock.json && npm install",
        "clean": "rm -rf dist && mkdir dist && deno fmt src",
        "prebuild": "yarn clean && yarn tsc || echo ok",
        "build": "echo ok",
        "xmin": "deno fmt dist && yarn esbuild dist/diff.ts --tree-shaking=\"ignore-annotations\" --minify  --minify-identifiers --platform=browser --minify-syntax --outfile=dist/diff.min.js"
    },
    "keywords": [
        "diff"
    ],
    "authors": [
        "Zoltan Erdos <zolika84@gmail.com>"
    ],
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/zed-vision/monorepo"
    },
    "homepage": "https://github.com/zed-vision/monorepo",
    "repository": "https://github.com/zed-vision/monorepo",
    "publishConfig": {
        "access": "public"
    },
    "gitHead": "af54e6ff16c9c20b6fba6365c211999fedeae2aa",
    "dependencies": {
        "@zedvision/sha256": "^10.13.13",
        "diff": "5.0.0",
        "esbuild": "0.8.26"
    },
    "devDependencies": {
        "typescript": "4.1.3"
    }
}
