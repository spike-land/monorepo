{
    "name": "@zedvision/diff",
    "version": "8.6.0",
    "description": "Diffs 2 strings",
    "main": "dist/diff.min.js",
    "private": false,
    "sideEffects": false,
    "scripts": {
        "npm:install": "rm -rf package-lock.json && npm install",
        "clean": "rm -rf dist && mkdir dist && deno fmt src",
        "build": "deno bundle --import-map ../../import_map.json --no-check --unstable src/diff.ts dist/diff.js && yarn min",
        "min": "deno fmt dist && yarn esbuild dist/diff.js --tree-shaking=\"ignore-annotations\" --minify  --minify-identifiers --minify-syntax --outfile=dist/diff.min.js"
    },
    "keywords": [
        "diff"
    ],
    "authors": [
        "Zoltan Erdos <zolika84@gmail.com>"
    ],
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/zed-vision/monorepo"
    },
    "homepage": "https://github.com/zed-vision/monorepo",
    "repository": "https://github.com/zed-vision/monorepo",
    "publishConfig": {
        "access": "public"
    },
    "gitHead": "779a0943d1492b82dc9b75186e4ec384f796c86a",
    "dependencies": {
        "diff": "5.0.0",
        "esbuild": "0.8.24"
    }
}
