{
  "name": "@spike.land/code-worker",
  "description": "An edge chat service that runs on Cloudflare Workers using Durable Objects",
  "version": "0.9.1",
  "main": "dist/index.js",
  "private": true,
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "yarn xbuild && yarn wrangler publish --env=testing dist/index.js",
    "prepublishOnly": "yarn wrangler publish",
    "xbuild": "esbuild  --bundle --tree-shaking=true --metafile=good.json --format=esm --loader:.html=text --external:__STATIC_CONTENT_MANIFEST --target=es2021 src/index.ts --outdir=dist"
  },
  "devDependencies": {
    "esbuild": "0.15.5"
  },
  "dependencies": {
    "@cloudflare/kv-asset-handler": "^0.2.0",
    "@cloudflare/workers-types": "3.14.1",
    "@spike.land/code": "^0.9.1",
    "typescript": "4.7.4",
    "wrangler": "2.0.26"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "db8e23b83d2dd82efc101eca9d1b5ec76d122df4"
}
