    "esbuild": "0.13.15",
    "ipfs-message-port-protocol": "0.10.3",
    "ipfs-message-port-server": "0.10.3",
    "ipfs-only-hash": "4.0.0",
    "uuid": "8.3.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
