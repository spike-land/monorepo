{
  "name": "@spike.land/code-worker",
  "description": "An edge chat service that runs on Cloudflare Workers using Durable Objects",
  "version": "0.8.4",
  "main": "dist/index.js",
  "private": true,
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "yarn wrangler publish --env=testing dist/index.js",
    "prepublishOnly": "yarn wrangler publish",
    "xbuild": "esbuild  --bundle --format=esm --loader:.html=text --external:__STATIC_CONTENT_MANIFEST --target=es2021 src/index.ts --outdir=dist"
  },
  "devDependencies": {
    "esbuild": "^0.15.0"
  },
  "dependencies": {
    "@cloudflare/kv-asset-handler": "^0.2.0",
    "@spike.land/code": "^0.8.4",
    "mime": "^3.0.0",
    "p2pcf": "^0.8.4",
    "typescript": "4.7.4",
    "wrangler": "2.0.25"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "db8e23b83d2dd82efc101eca9d1b5ec76d122df4"
}
