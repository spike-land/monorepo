{
  "name": "esbuild-worker",
  "version": "0.8.3",
  "module": "devserver.mjs",
  "type": "module",
  "scripts": {
    "build": "esbuild --bundle --sourcemap --outdir=dist --out-extension:.js=.mjs --external:__STATIC_CONTENT_MANIFEST --format=esm --target=esnext  worker/worker.ts ",
    "ts": "node -r esbuild-register",
    "dev:worker": "miniflare dist/worker.js --watch --debug --https - -open",
    "dev:vite": "vite",
    "build:vite": "tsc && vite build",
    "preview": "vite preview"
  },
  "license": "MIT",
  "devDependencies": {
    "@cloudflare/workers-types": "3.14.1",
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "@vitejs/plugin-react": "2.0.0",
    "esbuild": "0.14.49",
    "typescript": "4.7.4",
    "vite": "3.0.2"
  },
  "dependencies": {
    "miniflare": "2.6.0",
    "react": "18.2.0"
  }
}
