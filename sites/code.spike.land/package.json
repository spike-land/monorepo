  "devDependencies": {
    "@cloudflare/workers-types": "3.2.0",
    "@cloudflare/wrangler": "1.19.5",
    "@spike.land/cf-npm-site": "^0.0.99",
    "@spike.land/code": "^0.0.99",
    "esbuild": "0.13.14"
  },
  "private": true
}
