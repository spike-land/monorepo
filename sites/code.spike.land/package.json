  "version": "0.0.99",
  "module": "./dist/index.mjs",
  "scripts": {
    "deploy": "wrangler publish",
    "bundle": "esbuild --bundle --format=esm --loader:.html=text --target=esnext src/index.ts --outfile=dist/index.mjs",
    "postpublish": "yarn bundle"
