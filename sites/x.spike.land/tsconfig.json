{
  "compilerOptions": {
    "allowJs": true,
    "target": "esnext",
    "module": "esnext",
    "lib": ["es2020", "es2021", "we"],
    "alwaysStrict": true,
    "strict": true,
    "noEmit": true,
    "preserveConstEnums": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "types": ["@cloudflare/workers-types", "@types/websocket"]
  }
}
