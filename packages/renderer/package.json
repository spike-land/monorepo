{
  "name": "@zedvision/renderer",
  "version": "14.0.3",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "source": "./src/renderer.ts",
  "exports": "./dist/renderer.mjs",
  "main": "./dist/renderer.mjs",
  "sideEffects": false,
  "scripts": {
    "clean": "rm -rf dist .tsbuildinfo && deno fmt src",
    "build:renderer": "$(yarn bin esbuild) src/renderer.ts --bundle --format=esm --external:react --external:tslib --external:react-dom --platform=browser --define:process.env.NODE_ENV=\\\"production\\\" --outfile=dist/renderer.mjs",
    "build:renderer:umd": "$(yarn bin esbuild) src/renderer.ts --minify --bundle --format=iife --platform=browser  --define:process.env.NODE_ENV=\\\"production\\\"  --tree-shaking=ignore-annotations --target=es2017 --outfile=dist/renderer.umd.js",
    "build:draggableWindow": "$(yarn bin esbuild) src/DraggableWindow.tsx --minify --bundle --format=esm --platform=browser  --define:process.env.NODE_ENV=\\\"production\\\"  --tree-shaking=ignore-annotations --target=es2017 --outfile=dist/DraggableWindow.js",
    "build": "yarn build:renderer && cp -rf dist/* ../code/modules/"
  },
  "dependencies": {
    "@emotion/react": "11.4.0",
    "@emotion/styled": "11.3.0",
    "@material-ui/core": "5.0.0-alpha.32",
    "@types/react": "17.0.5",
    "@types/react-dom": "17.0.3",
    "esbuild": "0.11.19",
    "framer-motion": "4.1.13",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "tslib": "2.2.0",
    "typescript": "4.2.4"
  },
  "gitHead": "ca6fb1f76c1cc444bf6239deca1d6a6f1dd752d3"
}
