{
  "compilerOptions": {
    "allowJs": true,
    "alwaysStrict": true,
    "importHelpers": true,
    "downlevelIteration": true,
    "strict": true,
    "noEmit": false,
    "outDir": "dist",
    "allowSyntheticDefaultImports": true,
    "target": "esnext",
    "resolveJsonModule": true,
    "module": "esnext",
    "lib": ["esnext"],
    "types": ["@cloudflare/workers-types"]
  },
  "include": [
    "src/*",
    "../code/*"
  ]
}
