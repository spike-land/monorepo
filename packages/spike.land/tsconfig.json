{
  "compilerOptions": {
    "allowJs": true,
    "alwaysStrict": true,
    "importHelpers": true,
    "downlevelIteration": true,
    "strict": true,
    "noEmit": false,
    "outDir": "dist",
    "target": "ES2020",
    "resolveJsonModule": true,
    "module": "CommonJS",
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "Node",
    "lib": ["ES2020", "ES2020.String", "ESNext.Symbol","DOM", "DOM.Iterable","ESNext"],
    "types": ["@cloudflare/workers-types"]
  },
  "include": [
    "src/*"
  ]
}
