{
  "compilerOptions": {
    "allowJs": true,
    "alwaysStrict": true,
    "importHelpers": true,
    "downlevelIteration": true,
    "strict": true,
    "noEmit": false,
    "forceConsistentCasingInFileNames": true,
    "preserveConstEnums": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "dist",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "target": "ESNext",
    "module": "CommonJS",
    "lib": ["ESNext", "ES2015.Collection"],
    "types": ["@cloudflare/workers-types"]
  },

  "include": [
    "src/*"
  ]
}
