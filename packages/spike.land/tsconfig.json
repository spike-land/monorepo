{
  "compilerOptions": {
    "allowJs": true,
    "alwaysStrict": true,
    "strict": true,
    "declaration": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "importHelpers": true,
    "downlevelIteration": true,
    "forceConsistentCasingInFileNames": true,
    "preserveConstEnums": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "dist",
    "rootDir": ".",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "target": "ESNext",
    "module": "ESNext",
    "lib": ["ESNext"],
    "types": ["@cloudflare/workers-types", "@types/node"],
  },
  "exclude": ["**/*.spec.ts", "**/node_modules/*", "**/ .yarn", "**/dist"],
  "include": [
    "src"
, "../code/src/utils/diffUtils"  ]
}
