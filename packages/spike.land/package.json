{
  "name": "@spike-npm-land/code-worker",
  "description": "An edge chat service that runs on Cloudflare Workers using Durable Objects",
  "version": "0.9.56",
  "type": "module",
  "mpdule": "dist/index.js",
  "private": true,
  "scripts": {
    "dev": "wrangler dev",
    "dev:remote": "wrangler dev --remote",
    "// build": "yarn clean",
    "build:worker": "echo 'No build required - using direct deployment'",
    "deploy:dev": "wrangler deploy --env=testing --minify ./src/cf-workers.ts",
    "deploy:prod": "wrangler deploy --minify ./src/cf-workers.ts",
    "clean": "rimraf dist",
    "types:check": "tsc --noEmit",
    "test:unit": "vitest --no-color --reporter dot 2> ./error.txt run || touch ./error.txt && cat ./error.txt | grep -v .yarn | grep -v node_modules && rm ./error.txt",
    "test": "yarn types:check && yarn test:unit",
    "test:watch": "vitest"
  },
  "devDependencies": {
    "@cloudflare/kv-asset-handler": "0.3.4",
    "@cloudflare/vitest-pool-workers": "0.6.11",
    "@cloudflare/workers-types": "4.20250129.0",
    "@spike-npm-land/transpile": "0.9.56",
    "@types/mime-db": "1.43.5",
    "@types/mime-types": "2.1.4",
    "@typescript-eslint/parser": "8.22.0",
    "@vitest/runner": "2.1.0",
    "@vitest/snapshot": "2.1.0",
    "rimraf": "6.0.1",
    "typescript": "5.7.3",
    "vitest": "2.1.0",
    "wrangler": "3.107.2"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "0.36.3",
    "@clerk/backend": "1.23.10",
    "@clerk/clerk-sdk-node": "5.1.6",
    "@remix-run/cloudflare": "2.15.3",
    "@remix-run/react": "2.15.3",
    "@spike-npm-land/code": "0.9.56",
    "@types/node": "22.13.0",
    "async-mutex": "0.5.0",
    "avl": "1.5.3",
    "esbuild-wasm": "0.24.2",
    "esm-worker": "0.135.18",
    "hono": "4.6.20",
    "immutable": "5.0.3",
    "mime-db": "1.53.0",
    "mime-types": "2.1.35",
    "miniflare": "3.20250129.0",
    "openai": "4.82.0",
    "openai-edge": "1.2.2",
    "p-map": "7.0.3",
    "p-queue": "8.1.0",
    "replicate": "1.0.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "db8e23b83d2dd82efc101eca9d1b5ec76d122df4"
}
