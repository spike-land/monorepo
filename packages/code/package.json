{
  "name": "@spike.land/code",
  "version": "0.8.0",
  "description": "spike.land",
  "type": "module",
  "main": "./js/starter.ts",
  "files": [
    "**/*"
  ],
  "exports": {
    "./package.json": "./package.json",
    "./js/starter.ts": "./js/starter.ts",
    "./js/session": "./js/session.ts",
    "./js/esbuildEsm": "./js/esbuildEsm.ts",
    "./js/binary": "./js/binary.ts",
    "./js/importmap.json": "./js/importmap.json",
    "./js/mockedMap.json": "./js/mockedMap.json",
    "./shasums": "./shasums.ts"
  },
  "scripts": {
    "test": "uvu -r esbuild-register tests",
    "typecheck": "cd monorepo/packages/code && yarn tsc",
    "clean": "rm -rf .tsBuildInfo js/vendor ../../sites/spike.land/public && mkdir ../../sites/spike.land/public js/vendor && yarn favicons && yarn build:framer-motion && yarn build:emotion  && yarn build:preact && yarn importmap",
    "build:sw": "esbuild --outfile=../sites/spike.land/public/sw.js --platform=browser --bundle --minify ./js/sw.js && yarn sw",
    "start": "cd ../../.. && yarn start",
    "push": "cd ../../.. && yarn push",
    "build:preact": "esbuild --bundle js/preact.ts  --target=esnext --minify --format=esm --platform=browser  --outfile=../../sites/spike.land/public/react.mjs",
    "build:orbit-db": "esbuild --bundle js/preact.ts  --target=esnext --minify --format=esm --platform=browser  --outfile=../../sites/spike.land/public/react.mjs",
    "build:framer-motion": "yarn esbuild --bundle js/motion.tsx  --target=esnext --minify --format=esm  --platform=browser  --define:process.env.NODE_ENV=\\\"production\\\" --external:react --external:@emotion/is-prop-valid  --external:react --outfile=../../sites/spike.land/public/framer-motion.mjs",
    "build:emotion": "yarn esbuild --bundle js/emotion.tsx  --target=esnext --format=esm --external:react --external:react/jsx-runtime --platform=browser --define:process.env.NODE_ENV=\\\"production\\\" --outfile=../../sites/spike.land/public/emotion.mjs",
    "build": "yarn test && node esbuild-dev.mjs &&  yarn build:sw",
    "favicons": "cp -af js/assets/favicons/.  ../../sites/spike.land/public/ && mkdir  ../../sites/spike.land/public/assets && cp js/assets/synthwave.webp   ../../sites/spike.land/public/assets/",
    "importmap": "node --experimental-json-modules generate.mjs > ./importmap_NEW.json && mv importmap_NEW.json js/importmap.json",
    "workbox-cli": "workbox-cli",
    "sw": "echo ok",
    "xsw": "yarn workbox injectManifest workbox-config.json"
  },
  "keywords": [
    "monaco",
    "editor",
    "react",
    "typescript",
    "html",
    "vscode"
  ],
  "authors": [
    "Zoltan Erdos <zolika84@gmail.com>"
  ],
  "bugs": {
    "url": "https://github.com/spike-land/monorepo"
  },
  "homepage": "/",
  "repository": "https://github.com/spike-land/monorepo",
  "devDependencies": {
    "@jspm/generator": "1.0.0-beta.31",
    "@types/lodash": "4.14.182",
    "@types/node": "17.0.43",
    "typescript": "4.7.4",
    "vite": "2.9.12",
    "vite-plugin-import-maps": "0.1.2"
  },
  "author": "Zoltan Erdos <zolika84@gmail.com>",
  "license": "BSD-3-Clause",
  "publishConfig": {
    "access": "public"
  },
  "alias": {
    "stream": "stream-browserify",
    "react": "preact/compat",
    "react-dom": "preact/compat"
  },
  "resolutions": {
    "path": "path-browserify"
  },
  "dependencies": {
    "@emotion/cache": "^11.9.3",
    "@emotion/react": "^11.9.3",
    "@emotion/serialize": "^1.0.4",
    "@emotion/server": "^11.4.0",
    "@emotion/sheet": "^1.1.1",
    "@emotion/styled": "^11.9.3",
    "@emotion/utils": "^1.1.0",
    "@identifo/identifo-auth-js": "^3.3.2",
    "@mui/material": "^5.8.4",
    "@spike.land/qrious": "^0.8.0",
    "@types/prettier": "2.6.3",
    "@types/react": "18.0.14",
    "@types/react-dom": "18.0.5",
    "ace-builds": "1.6.0",
    "assert": "2.0.0",
    "async-mutex": "0.3.2",
    "buffer": "6.0.3",
    "core-js": "^3.23.1",
    "es-module-shims": "^1.5.6",
    "esbuild": "0.14.47",
    "esbuild-plugin-import-map": "2.1.0",
    "esbuild-plugin-jsximportsource": "^0.0.4",
    "esbuild-register": "^3.3.3",
    "esbuild-wasm": "0.14.47",
    "fast-diff": "1.2.0",
    "framer-motion": "6.3.12",
    "hoist-non-react-statics": "^3.3.2",
    "immutable": "4.1.0",
    "ipfs": "^0.63.3",
    "ipfs-core": "0.15.2",
    "ipfs-message-port-client": "0.12.2",
    "ipfs-message-port-server": "0.12.0",
    "lodash": "^4.17.21",
    "monaco-editor": "0.34.0-dev.20220620",
    "orbit-db": "0.28.6",
    "p-all": "^4.0.0",
    "path": "file:./path",
    "path-browserify": "file:./path",
    "popmotion": "^11.0.3",
    "preact": "^10.8.0",
    "preact-render-to-string": "^5.2.0",
    "prettier": "^2.7.0",
    "react": "npm:@preact/compat",
    "react-dom": "npm:@preact/compat",
    "shiki": "^0.10.1",
    "stream-browserify": "3.0.0",
    "util": "^0.12.4",
    "uvu": "^0.5.3"
  },
  "gitHead": "445587aa294dbba9f178dd8b7f2979878bd92a2c"
}
