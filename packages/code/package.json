    "types": "mkdir -p dts && tsc --emitDeclarationOnly || echo ok",
    "clean": "rm -rf .tsBuildInfo src/vendor dist dts",
    "start": "cd ../../.. && yarn start",
    "push": "cd ../../.. && yarn push",
    "lint": "eslint --ext .ts,.tsx src",
    "prebuild": "yarn test || echo FAILED",
