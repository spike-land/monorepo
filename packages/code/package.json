{
    "name": "@zedvision/code",
    "version": "8.4.6",
    "description": "zed.vison/code",
    "main": "dist/codeLoader.min.js",
    "files": [
        "**/*"
    ],
    "scripts": {
        "npm:install": "npm install",
        "clean": "rm -rf dist && mkdir dist && deno fmt src",
        "build:sw": "deno bundle  --unstable --import-map ../../import_map.json --no-check src/sw.ts sw.js",
        "build": "deno bundle --unstable --import-map ../../import_map.json --no-check src/codeBox.ts dist/codeLoader.js && yarn build:sw && yarn minify",
        "minify": "deno fmt dist && yarn esbuild dist/codeLoader.js --minify --outfile=dist/codeLoader.min.js"
    },
    "keywords": [
        "monaco",
        "editor",
        "react",
        "typescript",
        "html",
        "vscode"
    ],
    "authors": [
        "Zoltan Erdos <zolika84@gmail.com>"
    ],
    "bugs": {
        "url": "https://github.com/zed-vision/monorepo"
    },
    "homepage": "https://code.zed.vision",
    "repository": "https://github.com/zed-vision/monorepo",
    "devDependencies": {
        "esbuild": "0.8.22"
    },
    "gitHead": "fa29173ef1ea95fcc0914e5f7edd41ef10947a9f",
