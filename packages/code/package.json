{
  "name": "@spike.land/code",
  "version": "0.4.7",
  "description": "spike.land",
  "main": "./js/quickerStart.mjs",
  "files": [
    "**/*"
  ],
  "source": "./js/quickStart.mjs",
  "exports": {
    "./session": "./js/session.tsx",
    "./js/session": "./js/session.tsx",
    "./dist/bundle.mjs": "./js/dist/starter.bundle.mjs",
    "./dist/quickStart.mjs": "./js/dist/quickStart.mjs",
    "./dist/templates.mjs": "./js/dist/templates.mjs",
    "./package.json": "./package.json",
    "./importmap.json": "./js/importmap.json"
  },
  "scripts": {
    "clean": "rm -rf .tsBuildInfo js/dist && mkdir js/dist && deno fmt .",
    "bundle:starter": "deno bundle --import-map importmap.json js/starter.mjs dist/starter.bundle.mjs",
    "minify:bundle": "esbuild dist/starter.bundle.mjs --format=esm --target=es2018 --minify --tree-shaking=true --outfile=dist/bundle.mjs",
    "postinstall": "yarn importmap",
    "xxprepublishOnly": "yarn bundle:starter && yarn minify:bundle",
    "start": "cd ../.. && yarn start",
    "push": "cd ../.. && yarn push",
    "build": "rm -rf dist && yarn esbuild --bundle --format=esm js/templates.ts --outfile=dist/templates.mjs && node --experimental-json-modules esbuild-dev.mjs",
    "importmap": "node generate.mjs > ./js/importmap.json"
  },
  "keywords": [
    "monaco",
    "editor",
    "react",
    "typescript",
    "html",
    "vscode"
  ],
  "authors": [
    "Zoltan Erdos <zolika84@gmail.com>"
  ],
  "bugs": {
    "url": "https://github.com/spike-land/monorepo"
  },
  "homepage": "https://code.spike.land",
  "repository": "https://github.com/spike-land/monorepo",
  "devDependencies": {
    "esbuild": "0.14.8",
    "typescript": "4.5.4"
  },
  "gitHead": "85a0b3a41e4834872fa65570436bb43ba006a80d",
  "author": "Zoltan Erdos <zolika84@gmail.com>",
  "license": "BSD-3-Clause",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@emotion/styled": "^11.6.0",
    "@jspm/generator": "^1.0.0-beta.20",
    "@mui/material": "5.2.5",
    "@spike.land/qrious": "^0.4.0",
    "@spike.land/smart-monaco-editor": "^0.4.0",
    "@types/react": "17.0.38",
    "@types/react-dom": "17.0.11",
    "axax": "0.2.2",
    "comlink": "4.3.1",
    "es-module-shims": "1.4.1",
    "esbuild": "0.14.8",
    "esbuild-plugin-import-map": "^2.1.0",
    "framer-motion": "5.5.5",
    "immutable": "^4.0.0",
    "react": "18.0.0-rc.0-next-f2a59df48-20211208",
    "react-dom": "18.0.0-rc.0-next-f2a59df48-20211208",
    "textdiff-create": "1.0.5"
  }
}
