{
  "name": "@spike-npm-land/code",
  "version": "0.9.56",
  "type": "module",
  "description": "spike.land",
  "entry": "dist/modules.cjs",
  "main": "dist/modules.cjs",
  "source": "src/modules.ts",
  "types": "dts/src/modules.d.ts",
  "module": "dist/modules.mjs",
  "files": [
    "dist",
    "src",
    "dts"
  ],
  "scripts": {
    "codebase": "ai-digest -i $(pwd)/src --show-output-files",
    "fmt": "dprint fmt \"**/*.{ts,tsx,js,jsx,json,mjs}\"",
    "types": "tsc --build --verbose || echo ok",
    "types:cp": "cp -r dts/src/motion.d.ts dts/src/reactMod.d.ts dts/src/reactDom.d.ts dts/src/jsx.d.ts dts/src/emotion.d.ts dts/src/reactDomClient.d.ts dist/  && cp -r dts/src/@ dist/. ",
    "typecheck": "yarn types",
    "clean": "rm -rf .tsBuildInfo dist dts && mkdir -p dist dts dist-vite",
    "start": "cd ../../.. && yarn start",
    "push": "cd ../../.. && yarn push",
    "tw:x": "yarn tailwindcss -i src/app/globals.css -c tailwind.config.ts --postcss -m -o dist/app/tw-global.css",
    "sw": "echo ok",
    "test": "yarn vitest --no-color --reporter verbose run",
    "test-slim": "yarn typecheck && yarn lint &&  yarn vitest --no-color --reporter verbose run",
    "prebuild": "yarn test || echo 'Test FAILED during prebuild process'",
    "build": "yarn clean && yarn concurrently --kill-others-on-fail \"yarn build:vite\" \"yarn types && yarn types:cp\" \"npx -y tsx src/@/lib/esbuild-dev.ts\" && cp -a -f dist-vite/. dist/ && mkdir -p ./dist/app && cp ./dist/assets/index-*.css ./dist/app/tw-global.css && echo 'Build completed successfully!' || echo 'Build FAILED'",
    "dev": "vite",
    "build-and-clean:vite": "yarn x:build:vite && rm -rf dist-vite && mkdir dist-vite",
    "x:build:vite": "echo no vite",
    "build:vite": "vite build",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "keywords": [
    "monaco",
    "editor",
    "react",
    "typescript",
    "html",
    "vscode"
  ],
