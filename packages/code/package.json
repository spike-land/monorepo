{
  "name": "@spike-land/code",
  "version": "0.9.55",
  "type": "module",
  "description": "spike.land",
  "entry": "dist/modules.cjs",
  "main": "dist/modules.cjs",
  "source": "src/modules.ts",
  "types": "dts/src/modules.d.ts",
  "module": "dist/modules.mjs",
  "files": [
    "dist",
    "src",
    "dts"
  ],
  "scripts": {
    "codebase": "ai-digest -i $(pwd)/src --show-output-files",
    "fmt": "dprint fmt \"**/*.{ts,tsx,js,jsx,json,mjs}\"",
    "types": "tsc --emitDeclarationOnly || echo ok",
    "types:cp": "cp -r dts/src/motion.d.ts dts/src/reactMod.d.ts dts/src/reactDom.d.ts dts/src/jsx.d.ts dts/src/emotion.d.ts dts/src/reactDomClient.d.ts dist/  && cp -r dts/src/@ dist/. ",
    "typecheck": "tsc --noEmit --pretty false",
    "clean": "rm -rf .tsBuildInfo dist dts && mkdir -p dist dts",
    "start": "cd ../../.. && yarn start",
    "push": "cd ../../.. && yarn push",
    "lint": "eslint --cache  --no-color --quiet --fix",
    "tw": "yarn tailwindcss -i src/app/globals.css -c tailwind.config.ts --postcss -m -o dist/app/tw-global.css",
    "sw": "echo ok",
    "test": "yarn vitest --no-color --reporter verbose run",
    "test-slim": "yarn typecheck && yarn lint &&  yarn vitest --no-color --reporter verbose run",
    "prebuild": "yarn test || echo 'Test FAILED during prebuild process'",
    "build": "echo 'Starting build...' && yarn clean && yarn concurrently --kill-others-on-fail \"yarn types && yarn types:cp\" \"npx -y tsx esbuild-dev.ts\" \"yarn tw\" && echo 'Build completed successfully!' || echo 'Build FAILED'"
  },
  "keywords": [
    "monaco",
    "editor",
    "react",
    "typescript",
    "html",
    "vscode"
  ],
