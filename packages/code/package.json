  "scripts": {
    "test": "node --experimental-vm-modules $(yarn bin jest)",
    "codebase": "ai-digest -i $(pwd) --show-output-files",
    "fmt": "dprint fmt \"**/*.{ts,tsx,js,jsx,json,mjs}\"",
    "types": "mkdir -p dts && tsc --emitDeclarationOnly || echo ok",
    "clean": "rm -rf .tsBuildInfo src/vendor dist dts && yarn favicons",
    "start": "cd ../../.. && yarn start",
    "push": "cd ../../.. && yarn push",
    "lint": "eslint --ext .ts,.tsx src",
    "prebuild": "yarn test || echo FAILED",
    "build": "rm -rf dist dts && yarn types && yarn favicons && yarn es:build && cp -r dts/src/@/ dist/@/. ",
    "es:build": "echo EEEEEESSSSSSSSBBBBBBUUUUIIIILLLDDDDDD && deno run --allow-net --allow-read --allow-write --allow-env --allow-run esbuild-dev.ts",
    "favicons": "mkdir -p ./dist/assets && cp -af src/assets/. ./dist/assets/",
    "sw": "echo ok"
  },
