{
  "name": "@zedvision/emotion-react-renderer",
  "version": "11.10.18",
  "main": "src/index.js",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "source": "./src/renderer.ts",
  "exports": "./renderer",
  "sideEffects": false,
  "scripts": {
    "clean": "rm -rf dist .tsbuildinfo && deno fmt src",
    "build:renderer": "esbuild src/renderer.ts --minify --bundle --format=esm --platform=browser  --define:process.env.NODE_ENV=\\\"production\\\"  --tree-shaking=ignore-annotations --target=esnext --outfile=dist/renderer.js",
    "build:draggableWindow": "esbuild src/DraggableWindow.tsx --minify --bundle --format=esm --platform=browser  --define:process.env.NODE_ENV=\\\"production\\\"  --tree-shaking=ignore-annotations --target=esnext --outfile=dist/DraggableWindow.js",
    "prebuild": "yarn build:renderer && yarn build:draggableWindow",
    "build": "echo done in prebuld"
  },
  "devDependencies": {
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "esbuild": "0.8.34",
    "typescript": "4.2.0-dev.20210125"
  },
  "dependencies": {
    "@emotion/react": "11.1.4",
    "framer-motion": "3.2.2-rc.1",
    "react": "17.0.1",
    "react-dom": "17.0.1"
  }
}
