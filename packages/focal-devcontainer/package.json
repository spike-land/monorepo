{
  "name": "@devcontainer/focal-devcontainer",
  "tagname": "focal-devcontainer",
  "scripts": {
    "npm:install": "rm -rf package-lock.json && npm install",
    "docker:push": "docker push devimages/$npm_package_tagname:latest && docker push devimages/$npm_package_tagname:$npm_package_version",
    "docker:build": "yarn build && docker build . -t devimages/$npm_package_tagname:latest -t devimages/$npm_package_tagname:$npm_package_version",
    "build": "node index.js"
  },
  "files": [
    ".devcontainer/**/*",
    "docker-compose.yml",
    "Dockerfile",
    "README.md"
  ],
  "devDependencies": {
    "@devcontainer/generator": "^11.1.10"
  },
  "version": "11.1.10",
  "description": "devcontainer vscode docker xfce remoteDesktop",
  "main": "index.js",
  "repository": "git@github.com:zed-vision/monorepo.git",
  "author": "Zoltan Erdos <zolika84@gmail.com>",
  "license": "BSD-3-Clause",
  "private": false,
  "gitHead": "af54e6ff16c9c20b6fba6365c211999fedeae2aa",
  "publishConfig": {
    "access": "public"
  }
}
