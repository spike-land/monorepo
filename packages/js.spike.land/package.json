{
  "name": "@spike-land/transpile",
  "description": "An edge chat service that runs on Cloudflare Workers using Durable Objects",
  "version": "0.9.55",
  "type": "module",
  "main": "dist/index.js",
  "private": true,
  "scripts": {
    "dev": "wrangler dev",
    "clean": "rm -rf dist",
    "deploy": "yarn x:build && yarn deploy:dev",
    "deploy:dev": "wrangler deploy  src/index.ts",
    "prepublishOnly": "wrangler deploy src/index.ts",
    "predeploy": "yarn x:build",
    "x:build": "echo oook",
    "xs:build": "rm -rf ./src/esbuild.wasm && cp ../code/dist/esbuild*.wasm ./src/esbuild.wasm"
  },
  "devDependencies": {
    "@cloudflare/kv-asset-handler": "0.3.4",
    "@cloudflare/workers-types": "4.20240909.0",
    "typescript": "5.6.2",
    "wrangler": "3.77.0"
  },
  "dependencies": {
    "@spike-land/code": "0.9.55",
    "esbuild-wasm": "0.23.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "db8e23b83d2dd82efc101eca9d1b5ec76d122df4"
}
