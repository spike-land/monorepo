{
  "name": "esbuild-worker",
  "version": "0.9.2",
  "module": "devserver.mjs",
  "type": "module",
  "scripts": {
    "build": "npx esbuild --bundle --sourcemap --outdir=dist --out-extension:.js=.mjs --external:__STATIC_CONTENT_MANIFEST --format=esm --target=esnext  worker/worker.ts ",
    "ts": "node -r esbuild-register",
    "dev:worker": "miniflare dist/worker.js --watch --debug --https - -open",
    "dev:vite": "vite",
    "build:vite": "tsc && vite build",
    "preview": "vite preview"
  },
  "license": "MIT",
  "devDependencies": {
    "@cloudflare/workers-types": "4.20221111.0",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.9",
    "esbuild": "0.15.14"
  },
  "gitHead": "1aee37f582ca7efae8a35b8bc64ece3859bb07f4"
}
