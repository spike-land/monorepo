    "gatsby-plugin-mdx": "1.4.0",
    "gatsby-plugin-react-helmet": "3.4.0",
    "gatsby-plugin-remove-fingerprints": "0.0.2",
    "gatsby-plugin-remove-generator": "1.0.7",
    "gatsby-plugin-typography": "2.6.0",
