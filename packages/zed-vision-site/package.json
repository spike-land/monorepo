{
  "name": "@zed-vision/zed-vision-site",
  "scripts": {
    "postinstall": "gatsby telemetry --disable",
    "build": "yarn clean && deno fmt src && gatsby build --no-uglify && rm -rf ../../docs && mv public ../../docs",
    "start-https": "GATSBY_HOT_LOADER=fast-refresh yarn generate-certs && yarn gatsby develop --host 0.0.0.0 --https --cert-file .certs/localhost.cert --key-file .certs/localhost.key",
    "start": "gatsby develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "serve-https": "yarn generate-certs && file_server --cert .certs/localhost.cert --key .certs/localhost.key --host 0.0.0.0 ../../docs",
    "generate-certs": "test -d .certs || (mkdir .certs && cd .certs && npx devcert-cli generate localhost)"
  },
  "private": true,
  "description": "Zed vision blog",
  "version": "7.2.3",
  "author": "Zoltan Erdos <zolika84@gmail.com>",
  "bugs": {
    "url": "https://github.com/zed-vision/monorepo/"
  },
  "dependencies": {
    "@ampproject/worker-dom": "0.27.3",
    "@emotion/react": "11.1.1",
    "@emotion/styled": "11.0.0",
    "@emotion/styled-base": "11.0.0",
    "@zedvision/smart-monaco-editor": "^7.2.3",
    "avl": "1.4.4",
    "framer-motion": "2.9.4",
    "gatsby": "2.26.1",
    "html-format": "1.0.1",
    "react": "17.0.1",
    "react-diff-viewer": "3.1.1",
    "react-dom": "17.0.1",
    "react-helmet": "6.1.0",
    "react-json-tree": "0.13.0",
    "react-typography": "0.16.19",
    "typography": "0.16.19",
    "typography-theme-wordpress-2016": "0.16.19"
  },
  "homepage": "https://zed.vision",
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zed-vision/monorepo"
  },
