  "name": "@zedvision/zedvision-site",
  "scripts": {
    "develop": "gatsby develop",
    "gbuild": "cp -afr blog/. static && gatsby build --no-uglify && cp -afr blog/. static && rm -rf ../../docs* && cp -afr public/. ../../docs",
