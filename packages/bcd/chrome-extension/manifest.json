{
  "name": "The BCD experiment",
  "version": "1.1",
  "description": "playing with react and with chrome extensions",
  "permissions": ["browsingData", "background"],
  "background": {
    "scripts": ["tsc/background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "assets/icon.png",
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}
