{
  "name": "@spike.land/esm",
  "version": "0.0.87",
  "description": "esm modules for packages with maintainers from the 2015",
  "main": "dist/react.mjs",
  "files": [
    "**/*"
  ],
  "authors": [
    "Zoltan Erdos <zolika84@gmail.com>"
  ],
  "bugs": {
    "url": "https://github.com/spike-land/monorepo"
  },
  "exports": {
    "./react.mjs": "./dist/react.mjs",
    "./react-dom.mjs": "./dist/react-dom.mjs",
    "./react-is.mjs": "./dist/react-is.mjs",
    "./framer-motion.mjs": "./dist/framer-motion.mjs"
  },
  "license": "BSD-3-Clause",
  "dependencies": {
    "@types/react": "17.0.35",
    "@types/react-dom": "17.0.11",
    "esbuild": "0.13.14",
    "framer-motion": "5.3.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-is": "17.0.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "scripts": {
    "build": "yarn build:react && yarn build:react-is && yarn build:react-dom && yarn build:framer-motion && yarn build:ipfs-only-hash",
    "build:react": "yarn esbuild --bundle src/react.mjs --format=esm --outfile=dist/react.mjs",
    "build:react-is": "yarn esbuild --bundle src/react-is.mjs --format=esm --outfile=dist/react-is.mjs",
    "build:react-dom": "yarn esbuild --bundle src/react-dom.mjs --external:react --external:react-is  --format=esm --outfile=dist/react-dom.mjs",
    "build:ipfs-only-hash": "yarn esbuild --bundle src/ipfs-only-hash.mjs --external:tslib --format=esm --outfile=dist/ipfs-only-hash.mjs",
    "build:framer-motion": "yarn esbuild --bundle src/framer-motion.mjs --external:tslib --format=esm --external:react --external:@emotion/is-prop-valid --outfile=dist/framer-motion.mjs"
  },
  "private": false,
  "gitHead": "9a6c323d30cdcb35a077b26f0c6ae6e68e923229"
}
