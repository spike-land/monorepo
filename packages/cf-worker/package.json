{
    "name": "@zedvision/cf-worker",
    "version": "8.0.2",
    "description": "Worker script for zed.vision/code",
    "main": "dist/worker-script.min.js",
    "files": [
        "**/*"
    ],
    "scripts": {
        "dev": "yarn wrangler preview",
        "clean": "rm -rf dist && mkdir dist",
        "build": "yarn clean && deno fmt . && deno bundle src/index.ts dist/worker-script.js --no-check && yarn min && yarn deploy",
        "min": "yarn esbuild dist/worker-script.js  --minify --outfile=dist/worker-script.min.js",
        "deploy": "yarn wrangler publish"
    },
    "keywords": [
        "monaco",
        "editor",
        "react",
        "html",
        "vscode"
    ],
    "authors": [
        "Zoltan Erdos <zolika84@gmail.com>"
    ],
    "bugs": {
        "url": "https://github.com/zed-vision/monorepo"
    },
    "homepage": "https://code.zed.vision",
    "repository": "https://github.com/zed-vision/monorepo",
    "devDependencies": {
        "@cloudflare/workers-types": "2.1.0",
        "@cloudflare/wrangler": "1.12.2",
        "esbuild": "0.8.17"
    },
    "gitHead": "db133fb2edf1a37f0e340e26d8aa28c8763b93c2",
    "author": "Zoltan Erdos <zolika84@gmail.com>",
    "license": "BSD-3-Clause",
    "private": false,
    "publishConfig": {
        "access": "public"
    }
}
