{
    "name": "@zedvision/cf-worker",
    "version": "8.5.4",
    "description": "Worker script for zed.vision/code",
    "main": "dist/worker-script.min.js",
    "files": [
        "**/*"
    ],
    "scripts": {
        "npm:install": "rm -rf package-lock.json && npm install",
        "dev": "yarn build && yarn wrangler preview",
        "clean": "rm -rf dist && mkdir dist && deno fmt src",
        "build": "deno bundle --unstable --import-map ../../import_map.json src/index.ts dist/worker-script.js --no-check && yarn min",
        "min": "deno fmt dist && yarn esbuild dist/worker-script.js  --minify --outfile=dist/worker-script.min.js",
        "deploy": "yarn wrangler publish"
    },
    "keywords": [
        "monaco",
        "editor",
        "react",
        "html",
        "vscode"
    ],
    "authors": [
        "Zoltan Erdos <zolika84@gmail.com>"
    ],
    "bugs": {
        "url": "https://github.com/zed-vision/monorepo"
    },
    "homepage": "https://code.zed.vision",
    "repository": "https://github.com/zed-vision/monorepo",
    "devDependencies": {
        "@cloudflare/workers-types": "2.1.0",
        "@cloudflare/wrangler": "1.12.2",
        "@types/uuid": "8.3.0",
        "esbuild": "0.8.23"
    },
    "gitHead": "520741663e1da196b7dbbf0174e110548df04a31",
    "author": "Zoltan Erdos <zolika84@gmail.com>",
    "license": "BSD-3-Clause",
    "private": false,
    "publishConfig": {
        "access": "public"
    },
    "dependencies": {
        "uuid": "^8.3.2"
    }
}
