{
  "name": "@zedvision/emotion-react-renderer",
  "version": "13.0.4",
  "main": "src/index.js",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "source": "./src/renderer.ts",
  "exports": "./renderer",
  "sideEffects": false,
  "scripts": {
    "clean": "rm -rf dist .tsbuildinfo && deno fmt src",
    "build:renderer": "esbuild src/renderer.ts --minify --bundle --format=esm --platform=browser  --define:process.env.NODE_ENV=\\\"production\\\"  --tree-shaking=ignore-annotations --target=es2017 --outfile=dist/renderer.js",
    "build:renderer:umd": "esbuild src/renderer.ts --minify --bundle --format=iife --platform=browser  --define:process.env.NODE_ENV=\\\"production\\\"  --tree-shaking=ignore-annotations --target=es2017 --outfile=dist/renderer.umd.js",
    "build:draggableWindow": "esbuild src/DraggableWindow.tsx --minify --bundle --format=esm --platform=browser  --define:process.env.NODE_ENV=\\\"production\\\"  --tree-shaking=ignore-annotations --target=es2017 --outfile=dist/DraggableWindow.js",
    "prebuild": "yarn build:renderer && cp -rf dist/* ../code/modules/",
    "build": "echo done in prebuld"
  },
  "devDependencies": {
    "@types/react": "17.0.2",
    "@types/react-dom": "17.0.1",
    "esbuild": "0.8.54",
    "typescript": "4.2.2"
  },
  "dependencies": {
    "@emotion/react": "11.1.5",
    "@emotion/styled": "11.1.5",
    "@material-ui/core": "next",
    "@material-ui/icons": "next",
    "framer-motion": "3.9.0",
    "react": "17.0.1",
    "react-dom": "17.0.1"
  },
  "gitHead": "12815f92581c029760fde4f16e6a1e724795545a"
}
