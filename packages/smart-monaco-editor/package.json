{
    "name": "@zedvision/smart-monaco-editor",
    "version": "11.10.22",
    "description": "Monaco Editor with auto type downloads mostly for React",
    "sideEffects": false,
    "source": "./src/editor.ts",
    "exports": "./dist/editor.js",
    "scripts": {
        "prebuild": "yarn clean && tsc && deno fmt src",
        "build": "rm -rf ../code/js/smart-monaco-editor && mkdir -p ../code/modules/smart-monaco-editor && cp -ar dist ../code/modules/smart-monaco-editor/",
        "clean": "rm -rf dist tsconfig.tsbuildinfo && yarn format",
        "format": "deno fmt src"
    },
    "keywords": [
        "monaco",
        "editor",
        "react",
        "html",
        "vscode"
    ],
    "authors": [
        "Zoltan Erdos <zolika84@gmail.com>"
    ],
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/zed-vision/monorepo"
    },
    "homepage": "https://github.com/zed-vision/monorepo",
    "repository": "https://github.com/zed-vision/monorepo",
    "devDependencies": {
        "typescript": "4.2.0-dev.20210127"
    },
    "publishConfig": {
        "access": "public"
    },
    "gitHead": "12815f92581c029760fde4f16e6a1e724795545a"
}
