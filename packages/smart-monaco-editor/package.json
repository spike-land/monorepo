{
    "name": "@zedvision/smart-monaco-editor",
    "version": "8.7.4",
    "description": "Monaco Editor with auto type downloads mostly for React",
    "exports": "./lib/editor.js",
    "private": false,
    "sideEffects": false,
    "scripts": {
        "npm:install": "rm -rf package-lock.json && npm install",
        "prebuild": "deno fmt src && yarn tsc || echo done",
        "build": "echo done",
        "xprebuild": "deno fmt src && deno bundle --no-check --unstable --import-map ../../import_map.json src/editor.ts lib/editor.js",
        "xxbuild": "yarn min",
        "xxmin": "deno fmt dist && yarn esbuild --minify lib/editor.js --outfile=lib/editor.min.js ",
        "serve-https": "file_server --cert .certs/localhost.cert --key .certs/localhost.key --host 0.0.0.0 .",
        "clean": "rm -rf lib screenshot* && mkdir lib",
        "screenshots": "rm -rf screenshot* && node scripts/screenshots.js",
        "format": "deno fmt src"
    },
    "keywords": [
        "monaco",
        "editor",
        "react",
        "html",
        "vscode"
    ],
    "authors": [
        "Zoltan Erdos <zolika84@gmail.com>"
    ],
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/zed-vision/monorepo"
    },
    "homepage": "https://github.com/zed-vision/monorepo",
    "repository": "https://github.com/zed-vision/monorepo",
    "devDependencies": {
        "ace-builds": "1.4.12",
        "capture-website": "1.2.6",
        "monaco-editor": "0.21.2"
    },
    "publishConfig": {
        "access": "public"
    },
    "gitHead": "779a0943d1492b82dc9b75186e4ec384f796c86a",
    "dependencies": {
        "tslib": "2.0.3"
    }
}
