  },
  "devDependencies": {
    "@zedvision/dts-gen": "^10.14.1",
    "esbuild": "0.8.26",
    "qrious-core": "4.0.0"
  },
  "main": "dist/qrious.js",
  "source": "src/qrious.js",
  "exports": "./dist/qrious.esm.js",
  "scripts": {
    "npm:install": "rm -rf package-lock.json && yarn ipfs-npm install",
    "build:esm": "yarn esbuild src/QRious.js --loader:.css=text --bundle --minify --platform=browser --outfile=dist/qrious.esm.js --target=es2018 --format=esm",
    "build": "yarn build:esm",
    "dts": "yarn dts-gen -m qrious-core -f dist/qrious.d.ts"
  },
