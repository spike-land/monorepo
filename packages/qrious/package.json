{
  "name": "@zedvision/qrious",
  "version": "8.8.0",
  "description": "Library for QR code generation using canvas",
  "homepage": "https://github.com/neocotic/qrious",
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/neocotic/qrious/issues"
  },
  "sideEffects": false,
  "author": {
    "name": "Alasdair Mercer",
    "email": "mercer.alasdair@gmail.com",
    "url": "https://neocotic.com"
  },
  "license": "GPL-3.0",
  "keywords": [
    "qr",
    "code",
    "encode",
    "canvas",
    "image"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/zerdos/qrious.git"
  },
  "devDependencies": {
    "@zedvision/dts-gen": "^8.8.0",
    "esbuild": "0.8.26",
    "qrious-core": "4.0.0"
  },
  "main": "dist/qrious.js",
  "scripts": {
    "npm:install": "rm -rf package-lock.json && npm install",
    "build": "yarn esbuild src/QRious.js --loader:.css=text --bundle --outfile=dist/qrious.js --target=chrome58,firefox57,safari11,edge16 && yarn minify",
    "dts": "yarn dts-gen -m @zedvision/qrious -f dist/qrious.d.ts",
    "minify": "deno fmt dist && yarn esbuild --minify dist/qrious.js --outfile=dist/qrious.min.js"
  },
  "gitHead": "779a0943d1492b82dc9b75186e4ec384f796c86a"
}
