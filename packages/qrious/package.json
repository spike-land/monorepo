{
  "name": "@zedvision/qrious",
  "version": "13.0.2",
  "description": "Library for QR code generation using canvas",
  "homepage": "https://github.com/neocotic/qrious",
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/neocotic/qrious/issues"
  },
  "sideEffects": false,
  "author": {
    "name": "Alasdair Mercer",
    "email": "mercer.alasdair@gmail.com",
    "url": "https://neocotic.com"
  },
  "license": "GPL-3.0",
  "keywords": [
    "qr",
    "code",
    "encode",
    "canvas",
    "image"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/zerdos/qrious.git"
  },
  "devDependencies": {
    "esbuild": "0.8.46",
    "typescript": "^4.1.5"
  },  
  "dependencies": {
    "qrious-core": "4.0.0"
  },
  "typings": "dist/QRious.d.ts",
  "source": "src/QRious.tsx",
  "exports": "./dist/QRious.js",
  "scripts": {
    "build:esm": "esbuild src/QRious.js --loader:.css=text --bundle --minify --platform=browser --outfile=dist/QRious.js --target=es2017 --format=esm",
    "clean": "rm -rf dist && yarn tsc",
    "build": "yarn build:esm && cp -af ./dist/QRious.js ../code/modules"
  },
