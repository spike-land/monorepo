{
    "name": "@zedvision/code",
    "version": "2.3.4",
    "description": "Monaco Editor for React",
    "main": "code.zed-vision.min.js",
    "sideEffects": false,
    "scripts": {
        "clean": "rm code-box* code.zed*",
        "build": "deno fmt codeBox.ts && deno bundle codeBox.ts > code-box-bundle.js && yarn minify",
        "minify": "yarn esbuild code-box-bundle.js --minify --outfile=code.zed-vision.min.js",
        "serve": "file_server  ."     
    },
    "keywords": [
        "monaco",
        "editor",
        "react",
        "html",
        "vscode"
    ],
    "authors": [
        "Zoltan Erdos <zolika84@gmail.com>"
    ],
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/zed-vision/monorepo"
    },
    "homepage": "https://github.com/zed-vision/monorepo",
    "repository": "https://github.com/zed-vision/monorepo",
    "devDependencies": {
        "@types/react": "^16.9.53",
        "@types/webpack-env": "^1.15.3",
        "capture-website": "^1.2.4",
        "monaco-editor": "^0.21.2",
        "typescript": "^4.0.3",
        "esbuild": "0.7.21"
    }
}
