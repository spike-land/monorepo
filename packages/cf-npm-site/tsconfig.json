{
  "compilerOptions": {
    "allowJs": true,
    "target": "ESNext",
    "module": "ESNext",
    "lib": [
      "ESNext",
      "ESNext.Promise",
      "ES2015",
      "ES2016",
      "ES2017",
      "ES2018",
      "ES2019",
      "es2024",
      "ES2021",
      "DOM"
    ],
    "isolatedModules": true,
    "strict": true,
    "noEmit": false,
    "outDir": "dist",
    "declaration": true,
    "declarationDir": "dist",
    "preserveConstEnums": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "types": []
  }
}
