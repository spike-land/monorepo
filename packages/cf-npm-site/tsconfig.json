{
  "compilerOptions": {
    "allowJs": true,
    "target": "esnext",
    "module": "esnext",
    "lib": ["es2020"],
    "alwaysStrict": true,
    "strict": true,
    "noEmit": false,
    "outDir": "dist",
    "declaration": true,
    "declarationDir": "dist",
    "preserveConstEnums": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "types": ["@cloudflare/workers-types"]
  }
}
