  "main": "index.js",
  "devDependencies": {
    "@jspm/generator": "1.0.0-beta.21",
   "esbuild": "0.14.10",
    "lerna": "4.0.0",
    "open-cli": "7.0.1",
    "speed-limiter": "1.0.2",
    "yarn": "1.22.17",
    "rollup": "2.62.0",
    "@builder.io/qwik": "0.0.16-2",
    "@cloudflare/kv-asset-handler": "0.2.0",
    "@cloudflare/workers-types": "3.3.0",
    "@cloudflare/wrangler": "1.19.6",
    "@rollup/plugin-commonjs": "21.0.1",
    "@rollup/plugin-json": "4.1.0",
    "@rollup/plugin-node-resolve": "13.1.2",
    "@rollup/plugin-typescript": "8.3.0",
    "@types/service-worker-mock": "2.0.1",
    "concurrently": "7.0.0",

    "rimraf": "3.0.2",
    "rollup-plugin-terser": "7.0.2",
    "typescript": "4.5.4",
    "wait-on": "6.0.0"
  },
  "type": "module",
  "scripts": {
    "start": "killall node && node http2-server/http2-server.js & FAST_SERVE_PID=$! && npm run-script build && npm run open-cli https://localhost:8000/monorepo/packages/code/js/index.html --wait -- 'google chrome' --incognito",
    "clean": "npm run-script ws:yarn clean",
    "push": "npm run-script ws:yarn push",
    "deploy": "npm run-script ws:yarn deploy",
    "push:prod": "npm run-script ws:yarn push:prod",
    "re:build": "npm run-script ws:yarn re:build",
    "build": "npm run-script ws:yarn build",
    "postinstall": "npm run-script ws:yarn install && npm run-script ws:yarn lerna:init",
    "ws:yarn": "yarn --cwd monorepo ",
    "lerna": "lerna",
    "open-cli": "open-cli",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
