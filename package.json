{
  "name": "z",
  "version": "1.0.0",
  "description": "[Real time React page editor, in typescript! ![Screenshot](./spike.land-1920x1080.png)](https://code.spike.land)",
  "main": "index.js",
  "devDependencies": {
    "@jspm/generator": "^1.0.0-beta.21",
    "esbuild": "0.14.10",
    "lerna": "4.0.0",
    "open-cli": "7.0.1",
    "speed-limiter": "^1.0.2",
    "yarn": "^1.22.17"
  },
  "type": "module",
  "scripts": {
    "start": "node http2-server/http2-server.js & FAST_SERVE_PID=$! && npm run-script build && npm run open-cli https://localhost:8000/monorepo/packages/code/js/index.html --wait -- 'google chrome' --incognito && kill $FAST_SERVE_PID",
    "clean": "npm run-script ws:yarn clean",
    "push": "npm run-script ws:yarn push",
    "push:prod": "npm run-script ws:yarn push:prod",
    "re:build": "npm run-script ws:yarn re:build",
    "build": "npm run-script ws:yarn build",
    "postinstall": "npm run-script ws:yarn install",
    "ws:yarn": "yarn --cwd monorepo ",
    "lerna": "lerna",
    "open-cli": "open-cli",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
