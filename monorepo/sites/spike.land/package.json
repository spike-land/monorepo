{
  "name": "@spike.land/code-worker",
  "description": "An edge chat service that runs on Cloudflare Workers using Durable Objects",
  "version": "0.7.741",
  "main": "public/index.mjs",
  "type": "module",
  "module": "public/index.mjs",
  "private": true,
  "scripts": {
    "dev": "miniflare -m --wrangler-config wrangler.toml --https --host dev.spike.land --port 8080",
    "deploy": "wrangler publish src/index.ts",
    "xbuild": "esbuild --bundle --format=esm --loader:.html=text --target=esnext src/index.ts --outfile=public/index.mjs"
  },
  "devDependencies": {
    "@spike.land/cf-npm-site": "^0.7.717",
    "@spike.land/code": "^0.7.741",
    "tslib": "2.4.0",
    "typescript": "4.6.4"
  },
  "dependencies": {
    "@cloudflare/kv-asset-handler": "0.2.0",
    "@types/websocket": "1.0.5",
    "cookie": "0.5.0",
    "esbuild-wasm": "^0.14.38",
    "miniflare": "2.4.0",
    "textdiff-patch": "1.0.9"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "db8e23b83d2dd82efc101eca9d1b5ec76d122df4"
}
