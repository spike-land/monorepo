{
  "name": "@spike.land/code-worker",
  "description": "An edge chat service that runs on Cloudflare Workers using Durable Objects",
  "version": "0.7.966",
  "type": "module",
  "module": "dist/worker.mjs",
  "private": true,
  "scripts": {
    "dev": "miniflare -m --wrangler-config wrangler.toml --https --host 0.0.0.0 --port 8080",
    "deploy": "wrangler publish src/index.ts",
    "xbuild": "esbuild --bundle --format=esm --loader:.html=text --external:__STATIC_CONTENT_MANIFEST --target=esnext src/index.ts --outfile=dist/worker.mjs"
  },
  "devDependencies": {
    "@spike.land/cf-npm-site": "^0.7.959",
    "@spike.land/code": "^0.7.966",
    "tslib": "2.4.0",
    "typescript": "4.7.3"
  },
  "dependencies": {
    "@cloudflare/kv-asset-handler": "0.2.0",
    "@types/websocket": "1.0.5",
    "cookie": "0.5.0",
    "esbuild": "0.14.43",
    "esbuild-wasm": "0.14.43",
    "globals": "13.15.0",
    "miniflare": "2.5.0",
    "prettier": "^2.6.2",
    "textdiff-patch": "1.0.9"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "db8e23b83d2dd82efc101eca9d1b5ec76d122df4"
}
