    "bundle": "esbuild --bundle --format=esm --minify --tree-shaking=true --loader:.html=text --target=esnext src/index.ts --outfile=public/index.mjs",
    "postpublish": "yarn bundle"
  },
  "devDependencies": {
    "@spike.land/cf-npm-site": "^0.7.808",
    "@spike.land/code": "^0.7.867"
  },
  "private": true,
  "dependencies": {
    "@types/websocket": "1.0.5",
    "esbuild-wasm": "0.14.42",
    "textdiff-patch": "1.0.9"
  }
}
