{
  "name": "esbuild-worker",
  "version": "0.7.964",
  "module": "devserver.mjs",
  "type": "module",
  "scripts": {
    "build": "esbuild --bundle --sourcemap --outdir=dist --out-extension:.js=.mjs --external:__STATIC_CONTENT_MANIFEST --format=esm --target=esnext  worker/worker.ts ",
    "ts": "node -r esbuild-register",
    "dev:worker": "miniflare dist/worker.js --watch --debug --https - -open",
    "dev:vite": "vite",
    "build:vite": "tsc && vite build",
    "preview": "vite preview"
  },
  "license": "MIT",
  "devDependencies": {
    "@cloudflare/workers-types": "3.11.0",
    "@types/react": "^18.0.12",
    "@types/react-dom": "^18.0.5",
    "@vitejs/plugin-react": "^1.3.2",
    "esbuild": "^0.14.43",
    "typescript": "^4.7.3",
    "vite": "2.9.12"
  },
  "dependencies": {
    "miniflare": "^2.5.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0"
  }
}
