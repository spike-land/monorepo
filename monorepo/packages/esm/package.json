{
  "name": "@spike.land/esm",
  "version": "0.6.204",
  "description": "esm modules for packages with maintainers from the 2015",
  "files": [
    "**/*"
  ],
  "authors": [
    "Zoltan Erdos <zolika84@gmail.com>"
  ],
  "bugs": {
    "url": "https://github.com/spike-land/monorepo"
  },
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    "./react.mjs": "./dist/react.mjs",
    "./react-is.mjs": "./dist/react-is.mjs",
    "./framer-motion.mjs": "./dist/framer-motion.mjs",
    "./textdiff-create.mjs": "./dist/textdiff-create.mjs",
    "./textdiff-patch.mjs": "./dist/textdiff-patch.mjs"
  },
  "typings": "dist/types/*",
  "license": "BSD-3-Clause",
  "dependencies": {
    "@types/react": "17.0.39",
    "framer-motion": "6.2.4",
    "hoist-non-react-statics": "^3.3.2",
    "preact": "^10.6.5",
    "react-is": "^17.0.2",
    "textdiff-create": "1.0.7",
    "textdiff-patch": "1.0.6"
  },
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "scripts": {
    "clean": "rm -rf dist",
    "build": "yarn build:framer-motion & yarn build:react & yarn build:emotion-react",
    "build:react": "yarn esbuild --bundle src/react.ts --format=esm --outfile=dist/react.mjs",
    "build:emotion-react": "yarn esbuild --bundle --define:process.env.NODE_ENV=\"production\"  src/emotion-react.ts  --external:react  --tree-shaking=true  --format=esm --outfile=dist/emotion-react.mjs",
    "build:framer-motion": "yarn esbuild --bundle src/framer-motion.mjs --format=esm --external:react --external:@emotion/is-prop-valid --outfile=dist/framer-motion.mjs",
    "xbuild:framer-motion": "yarn esbuild --bundle src/framer-motion.mjs --minify --tree-shaking=true --external:tslib --format=esm --external:react --external:@emotion/is-prop-valid --outfile=dist/framer-motion.mjs",
    "dts:framer-motion": "rollup -c framer-motion.rollup.config.js",
    "build:textdiff-create": "yarn esbuild --bundle src/textdiff-create.mjs --minify --tree-shaking=true --format=esm  --outfile=dist/textdiff-create.mjs",
    "build:textdiff-patch": "yarn esbuild --bundle src/textdiff-patch.mjs --minify --tree-shaking=true --format=esm  --outfile=dist/textdiff-patch.mjs"
  },
  "private": false,
  "gitHead": "f4913f9918da7ff533b63f66efd8bcbdf0f3afc7"
}
