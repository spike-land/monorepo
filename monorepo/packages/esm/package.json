{
  "name": "@spike.land/esm",
  "version": "0.6.108",
  "description": "esm modules for packages with maintainers from the 2015",
  "files": [
    "**/*"
  ],
  "authors": [
    "Zoltan Erdos <zolika84@gmail.com>"
  ],
  "bugs": {
    "url": "https://github.com/spike-land/monorepo"
  },
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    "./react.mjs": "./dist/react.mjs",
    "./react-dom.mjs": "./dist/react-dom.mjs",
    "./react-is.mjs": "./dist/react-is.mjs",
    "./framer-motion.mjs": "./dist/framer-motion.mjs",
    "./textdiff-create.mjs": "./dist/textdiff-create.mjs",
    "./textdiff-patch.mjs": "./dist/textdiff-patch.mjs"
  },
  "typings": "dist/types/*",
  "license": "BSD-3-Clause",
  "dependencies": {
    "@types/react": "17.0.38",
    "@types/react-dom": "17.0.11",
    "framer-motion": "6.2.3",
    "react": "next",
    "react-dom": "next",
    "react-is": "next",
    "textdiff-create": "1.0.7",
    "textdiff-patch": "1.0.5"
  },
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "scripts": {
    "clean": "rm -rf dist",
    "build": "yarn build:framer-motion && yarn build:react && yarn build:emotion-react &&  yarn build:react-dom && yarn build:react-is &&  yarn build:textdiff-create && yarn build:textdiff-patch",
    "build:react": "yarn esbuild --bundle --minify --tree-shaking=true src/react.ts --format=esm --outfile=dist/react.mjs",
    "build:emotion-react": "yarn esbuild --bundle --minify --tree-shaking=true src/emotion-react.mjs --format=esm --outfile=dist/emotion-react.mjs",
    "build:react-is": "yarn esbuild --bundle --minify --tree-shaking=true  src/react-is.mjs --format=esm --outfile=dist/react-is.mjs",
    "xdeno:react-dom": " deno bundle https://ga.jspm.io/npm:react-dom@18.0.0-rc.0-next-13036bfbc-20220121/index.js --import-map ../code/importmap.json dist/react-dom-deno.js",
    "build:react-dom": "yarn esbuild --bundle src/react-dom.mjs --minify --tree-shaking=true --external:react --external:react-is  --format=esm --outfile=dist/react-dom.mjs",
    "xbuild:react-dom": "yarn esbuild --bundle dist/react-dom-deno.js --external:react --external:react-is  --format=esm --outfile=dist/react-dom.mjs",
    "build:framer-motion": "yarn esbuild --bundle src/framer-motion.mjs  --minify --tree-shaking=true --format=esm --external:react --external:@emotion/is-prop-valid --outfile=dist/framer-motion.mjs",
    "xbuild:framer-motion": "yarn esbuild --bundle src/framer-motion.mjs --minify --tree-shaking=true --external:tslib --format=esm --external:react --external:@emotion/is-prop-valid --outfile=dist/framer-motion.mjs",
    "dts:framer-motion": "rollup -c framer-motion.rollup.config.js",
    "build:textdiff-create": "yarn esbuild --bundle src/textdiff-create.mjs --minify --tree-shaking=true --format=esm  --outfile=dist/textdiff-create.mjs",
    "build:textdiff-patch": "yarn esbuild --bundle src/textdiff-patch.mjs --minify --tree-shaking=true --format=esm  --outfile=dist/textdiff-patch.mjs"
  },
  "private": false,
  "gitHead": "f19ed65bbe1e6fa6921e9d4a07d42a6cc4003eac"
}
