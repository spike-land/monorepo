    "esbuild-wasm": "0.14.30",
    "framer-motion": "6.2.8",
    "hoist-non-react-statics": "^3.3.2",
    "immutable": "^4.0.0",
    "lodash": "^4.17.21",
    "monaco-editor": "0.33.0",
    "monaco-jsx-highlighter": "^2.0.4",
