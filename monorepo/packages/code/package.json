{
  "name": "@spike.land/code",
  "version": "0.6.49",
  "description": "spike.land",
  "main": "./js/dist/quickStart.mjs",
  "files": [
    "**/*"
  ],
  "source": "./js/quickStart.mjs",
  "exports": {
    "./package.json": "./package.json",
    "./js/dist/quickStart.mjs": "./js/dist/quickStart.mjs",
    "./js/dist/quickStart.bundle.mjs": "./js/dist/quickStart.bundle.mjs",
    "./js/dist/mui.mjs": "./js/dist/mui.mjs",
    "./js/dist/startMonaco.mjs": "./js/dist/startMonaco.mjs",
    "./js/dist/session.mjs": "./js/dist/session.mjs",
    "./js/dist/renderPreviewWindow.mjs": "./js/dist/renderPreviewWindow.mjs",
    "./js/session.tsx": "./js/session.tsx",
    "./js/importmap.json": "./js/importmap.json"
  },
  "scripts": {
    "clean": "rm -rf .tsBuildInfo js/dist && mkdir js/dist && deno fmt .",
    "xpostinstall": "yarn importmap",
    "build:sw": "yarn esbuild --outfile=./js/sw.js --bundle js/sw.ts && yarn sw",
    "mui:deno": "deno bundle --import-map ./js/importmap.json ./js/dist/mui.mjs ./js/vendor/mui.mjs",
    "quickStart:deno": "deno bundle --import-map ./js/importmap.json ./js/dist/quickStart.mjs ./js/dist/quickStart.bundle.mjs --no-check",
    "renderToString:deno": "deno bundle --import-map ./js/importmap.json ./js/renderToString.tsx ./js/vendor/renderToString.mjs --no-check",
    "start": "cd ../.. && yarn start",
    "push": "cd ../.. && yarn push",
    "build": "yarn clean &&  node --experimental-json-modules esbuild-dev.mjs && yarn build:sw",
    "importmap": "node --experimental-json-modules generate.mjs > ./importmap_NEW.json && mv importmap_NEW.json js/importmap.json",
    "workbox-cli": "workbox-cli",
    "sw": "yarn workbox injectManifest workbox-config.js"
  },
  "keywords": [
    "monaco",
    "editor",
    "react",
    "typescript",
    "html",
    "vscode"
  ],
  "authors": [
    "Zoltan Erdos <zolika84@gmail.com>"
  ],
  "bugs": {
    "url": "https://github.com/spike-land/monorepo"
  },
  "homepage": "https://spike.land",
  "repository": "https://github.com/spike-land/monorepo",
  "devDependencies": {
    "typescript": "4.5.4"
  },
  "gitHead": "f3d54206eba35bbaa187a8c7425680b9770d1354",
  "author": "Zoltan Erdos <zolika84@gmail.com>",
  "license": "BSD-3-Clause",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@emotion/cache": "^11.7.1",
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@spike.land/code": "^0.6.49",
    "@spike.land/qrious": "^0.6.31",
    "@spike.land/smart-monaco-editor": "^0.6.49",
    "@types/react": "17.0.38",
    "@types/react-dom": "17.0.11",
    "async-mutex": "^0.3.2",
    "comlink": "4.3.1",
    "es-module-shims": "1.4.1",
    "esbuild-plugin-import-map": "^2.1.0",
    "framer-motion": "5.6.0",
    "immutable": "^4.0.0",
    "lodash": "^4.17.21",
    "react": "next",
    "react-dom": "next",
    "workbox-cacheable-response": "6.4.2",
    "workbox-cli": "6.4.2",
    "workbox-precaching": "6.4.2",
    "workbox-routing": "6.4.2",
    "workbox-strategies": "6.4.2",
    "workbox-sw": "6.4.2"
  }
}
