{
  "name": "@spike.land/code",
  "version": "0.7.469",
  "description": "spike.land",
  "main": "./js/dist/starter.mjs",
  "files": [
    "**/*"
  ],
  "exports": {
    "./package.json": "./package.json",
    "./js/dist/starter.mjs": "./js/dist/starter.mjs",
    "./js/session.tsx": "./js/session.tsx",
    "./js/binary.ts": "./js/binary.ts",
    "./js/importmap.json": "./js/importmap.json",
    "./js/mockedMap.json": "./js/mockedMap.json",
    "./shasums": "./shasums.ts"
  },
  "scripts": {
    "clean": "rm -rf .tsBuildInfo js/vendor js/dist && mkdir js/dist js/vendor && deno fmt js && yarn build:framer-motion && yarn build:emotion  && yarn build:preact",
    "xpostinstall": "yarn importmap",
    "build:sw": "yarn esbuild --outfile=./js/sw.js --platform=browser --bundle --minify js/sw.ts && yarn sw",
    "start": "cd ../../.. && yarn start",
    "push": "cd ../../.. && yarn push",
    "build:preact": "deno bundle --importmap js/importmap.json preact.mjs --unstable > js/dist/react.mjs",
    "build:framer-motion": "yarn esbuild --bundle js/motion.tsx  --target=es2017 --format=esm  --platform=browser  --define:process.env.NODE_ENV=\\\"production\\\" --external:react --external:@emotion/is-prop-valid --outfile=js/dist/framer-motion.mjs",
    "build:emotion": "yarn esbuild --bundle js/emotion.tsx  --target=es2017 --format=esm --external:react --platform=browser --define:process.env.NODE_ENV=\\\"production\\\" --outfile=js/dist/emotion.mjs",
    "build": "node --experimental-json-modules esbuild-dev.mjs && yarn build:sw",
    "importmap": "node --experimental-json-modules generate.mjs > ./importmap_NEW.json && mv importmap_NEW.json js/importmap.json",
    "xworkbox-cli": "workbox-cli",
    "sw": "echo ok",
    "xsw": "yarn workbox injectManifest workbox-config.json"
  },
  "keywords": [
    "monaco",
    "editor",
    "react",
    "typescript",
    "html",
    "vscode"
  ],
  "authors": [
    "Zoltan Erdos <zolika84@gmail.com>"
  ],
  "bugs": {
    "url": "https://github.com/spike-land/monorepo"
  },
  "homepage": "https://spike.land",
  "repository": "https://github.com/spike-land/monorepo",
  "devDependencies": {
    "typescript": "4.6.3",
    "vite": "2.9.1",
    "vite-plugin-import-maps": "0.1.2"
  },
  "gitHead": "cf1f5d25e26ea72ce197cf6dc7b5f5d49919373d",
  "author": "Zoltan Erdos <zolika84@gmail.com>",
  "license": "BSD-3-Clause",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@babel/core": "^7.17.8",
    "@babel/parser": "^7.17.8",
    "@babel/runtime": "7.17.8",
    "@babel/standalone": "7.17.8",
    "@babel/traverse": "^7.17.3",
    "@chialab/esbuild-plugin-jsx-import": "^0.15.9",
    "@emotion/babel-plugin": "^11.7.2",
    "@emotion/cache": "^11.7.1",
    "@emotion/react": "^11.8.2",
    "@emotion/serialize": "^1.0.2",
    "@emotion/sheet": "^1.1.0",
    "@emotion/styled": "^11.8.1",
    "@emotion/utils": "^1.1.0",
    "@emotion/weak-memoize": "0.2.5",
    "@mui/material": "^5.5.3",
    "@spike.land/qrious": "^0.7.455",
    "@types/babel__core": "^7.1.19",
    "@types/babel__standalone": "^7.1.4",
    "@types/isomorphic-git__lightning-fs": "^4.4.3",
    "@types/prettier": "^2.4.4",
    "@types/react": "17.0.43",
    "@types/react-dom": "^17.0.14",
    "async-mutex": "0.3.2",
    "buffer": "^6.0.3",
