{
  "name": "@spike.land/code",
  "version": "0.7.693",
  "description": "spike.land",
  "type": "module",
  "main": "./js/public/starter.mjs",
  "files": [
    "**/*"
  ],
  "exports": {
    "./package.json": "./package.json",
    "./js/public/starter.mjs": "./js/public/starter.mjs",
    "./js/session": "./js/session.ts",
    "./js/binary": "./js/binary.ts",
    "./js/importmap.json": "./js/importmap.json",
    "./js/mockedMap.json": "./js/mockedMap.json",
    "./shasums": "./shasums.ts"
  },
  "scripts": {
    "typecheck": "cd monorepo/packages/code && yarn tsc",
    "clean": "rm -rf .tsBuildInfo js/vendor js/public && mkdir js/public js/vendor && deno fmt js && yarn favicons && yarn build:framer-motion && yarn build:emotion  && yarn build:preact",
    "xpostinstall": "yarn importmap",
    "build:sw": "esbuild --outfile=./js/public/sw.js --platform=browser --bundle --minify ./js/sw.js && yarn sw",
    "start": "cd ../../.. && yarn start",
    "push": "cd ../../.. && yarn push",
    "build:preact": "esbuild --bundle js/preact.ts  --target=esnext --minify --format=esm --platform=browser  --outfile=js/public/react.mjs",
    "build:framer-motion": "yarn esbuild --bundle js/motion.tsx  --target=esnext --minify --format=esm  --platform=browser  --define:process.env.NODE_ENV=\\\"production\\\" --external:react --external:@emotion/is-prop-valid  --external:react --external:tslib --outfile=js/public/framer-motion.mjs",
    "build:emotion": "yarn esbuild --bundle js/emotion.tsx  --target=esnext --format=esm --external:react --external:react/jsx-runtime --platform=browser --external:tslib --define:process.env.NODE_ENV=\\\"production\\\" --outfile=js/public/emotion.mjs",
    "build": "node esbuild-dev.mjs &&  yarn build:sw",
    "favicons": "cp -a -f js/assets/favicons/. js/public",
    "importmap": "node --experimental-json-modules generate.mjs > ./importmap_NEW.json && mv importmap_NEW.json js/importmap.json",
    "workbox-cli": "workbox-cli",
    "sw": "echo ok",
    "xsw": "yarn workbox injectManifest workbox-config.json"
  },
  "keywords": [
    "monaco",
    "editor",
    "react",
    "typescript",
    "html",
    "vscode"
  ],
  "authors": [
    "Zoltan Erdos <zolika84@gmail.com>"
  ],
  "bugs": {
    "url": "https://github.com/spike-land/monorepo"
  },
  "homepage": "https://spike.land",
  "repository": "https://github.com/spike-land/monorepo",
  "devDependencies": {
    "@types/lodash": "4.14.182",
    "@types/node": "17.0.27",
    "typescript": "^4.6.4",
    "vite": "2.9.7",
    "vite-plugin-import-maps": "0.1.2"
  },
  "gitHead": "cf1f5d25e26ea72ce197cf6dc7b5f5d49919373d",
  "author": "Zoltan Erdos <zolika84@gmail.com>",
  "license": "BSD-3-Clause",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@babel/core": "^7.17.9",
    "@babel/parser": "^7.17.9",
    "@babel/runtime": "7.17.9",
    "@babel/standalone": "7.17.11",
    "@babel/traverse": "^7.17.9",
    "@emotion/babel-plugin": "^11.9.2",
    "@emotion/cache": "^11.7.1",
    "@emotion/react": "^11.9.0",
    "@emotion/serialize": "^1.0.3",
    "@emotion/sheet": "^1.1.0",
    "@emotion/styled": "^11.8.1",
    "@emotion/utils": "^1.1.0",
    "@emotion/weak-memoize": "0.2.5",
    "@mui/material": "^5.6.2",
    "@spike.land/qrious": "^0.7.644",
    "@types/babel__core": "^7.1.19",
    "@types/babel__standalone": "^7.1.4",
    "@types/isomorphic-git__lightning-fs": "^4.4.3",
    "@types/preact-i18n": "^2.3.1",
    "@types/prettier": "^2.6.0",
    "@types/react": "18.0.8",
    "@types/react-dom": "^18.0.2",
    "async-mutex": "0.3.2",
    "buffer": "^6.0.3",
    "comlink": "4.3.1",
    "core-js": "^3.22.2",
    "es-module-shims": "1.5.4",
    "esbuild": "0.14.38",
    "esbuild-plugin-import-map": "2.1.0",
    "esbuild-plugin-jsximportsource": "^0.0.4",
    "esbuild-wasm": "0.14.38",
    "framer-motion": "6.3.3",
    "hoist-non-react-statics": "^3.3.2",
    "immutable": "^4.0.0",
    "lodash": "^4.17.21",
    "monaco-editor": "0.34.0-dev.20220503",
    "monaco-jsx-highlighter": "^2.0.4",
    "p-all": "^4.0.0",
    "path-browserify": "^1.0.1",
    "popmotion": "^11.0.3",
    "preact": "^10.7.1",
    "preact-render-to-string": "^5.1.21",
    "prettier": "^2.6.2",
    "react": "npm:@preact/compat@17.0.3",
    "regenerator-runtime": "0.13.9",
    "stream-browserify": "^3.0.0",
    "textdiff-create": "1.0.9",
    "textdiff-patch": "1.0.9",
    "workbox-cacheable-response": "6.5.3",
    "workbox-cli": "6.5.3",
    "workbox-precaching": "6.5.3",
    "workbox-routing": "6.5.3",
    "workbox-strategies": "6.5.3",
    "workbox-sw": "6.5.3",
    "workbox-window": "6.5.3"
  }
}
