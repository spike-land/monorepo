{
  "name": "@spike.land/code",
  "version": "0.7.113",
  "description": "spike.land",
  "main": "./js/dist/starter.mjs",
  "files": [
    "**/*"
  ],
  "exports": {
    "./package.json": "./package.json",
    "./js/dist/starter.mjs": "./js/dist/starter.mjs",
    "./js/session.tsx": "./js/session.tsx",
    "./js/binary.ts": "./js/binary.ts",
    "./js/importmap.json": "./js/importmap.json",
    "./js/dist/react.mjs": "./js/dist/react.mjs",
    "./shasums": "./shasums.ts"
  },
  "scripts": {
    "clean": "rm -rf .tsBuildInfo js/vendor js/dist && mkdir js/dist js/vendor && deno fmt js && yarn build:framer-motion && yarn build:emotion  && yarn build:preact",
    "xpostinstall": "yarn importmap",
    "build:sw": "yarn esbuild --outfile=./js/sw.js --platform=browser --bundle --minify js/sw.ts && yarn sw",
    "start": "cd ../../.. && yarn start",
    "push": "cd ../../.. && yarn push",
    "build:preact": "deno bundle --importmap js/importmap.json js/preact.ts --unstable > js/dist/react.mjs",
    "build:framer-motion": "yarn esbuild --bundle js/motion.tsx  --target=es2017 --format=esm  --platform=browser  --define:process.env.NODE_ENV=\\\"production\\\" --external:react --external:@emotion/is-prop-valid --outfile=js/dist/framer-motion.mjs",
    "build:emotion": "yarn esbuild --bundle js/emotion.tsx  --target=es2017 --format=esm --external:react --platform=browser --define:process.env.NODE_ENV=\\\"production\\\" --outfile=js/dist/emotion.mjs",
    "build": "node --experimental-json-modules esbuild-dev.mjs  && yarn build:sw",
    "importmap": "node --experimental-json-modules generate.mjs > ./importmap_NEW.json && mv importmap_NEW.json js/importmap.json",
    "workbox-cli": "workbox-cli",
    "sw": "yarn workbox injectManifest workbox-config.json"
  },
  "keywords": [
    "monaco",
    "editor",
    "react",
    "typescript",
    "html",
    "vscode"
  ],
  "authors": [
    "Zoltan Erdos <zolika84@gmail.com>"
  ],
  "bugs": {
    "url": "https://github.com/spike-land/monorepo"
  },
  "homepage": "https://spike.land",
  "repository": "https://github.com/spike-land/monorepo",
  "devDependencies": {
    "typescript": "4.5.5"
  },
  "gitHead": "cf1f5d25e26ea72ce197cf6dc7b5f5d49919373d",
  "author": "Zoltan Erdos <zolika84@gmail.com>",
  "license": "BSD-3-Clause",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@babel/runtime": "^7.17.0",
    "@babel/standalone": "^7.17.1",
    "@emotion/cache": "^11.7.1",
    "@emotion/react": "^11.7.1",
    "@emotion/serialize": "^1.0.2",
    "@emotion/sheet": "^1.1.0",
    "@emotion/styled": "^11.6.0",
    "@emotion/utils": "^1.0.0",
    "@emotion/weak-memoize": "^0.2.5",
    "@isomorphic-git/lightning-fs": "isomorphic-git/lightning-fs",
    "@mui/material": "^5.4.0",
    "@spike.land/qrious": "^0.7.52",
    "@types/babel__standalone": "^7.1.4",
    "@types/isomorphic-git__lightning-fs": "^4.4.2",
    "@types/prettier": "^2.4.3",
    "@types/react": "17.0.39",
    "@types/react-dom": "^17.0.11",
    "async-mutex": "^0.3.2",
    "comlink": "4.3.1",
    "es-module-shims": "1.4.5",
    "esbuild": "0.14.21",
    "esbuild-plugin-import-map": "2.1.0",
    "esbuild-wasm": "^0.14.20",
    "framer-motion": "6.2.6",
    "hoist-non-react-statics": "^3.3.2",
    "immutable": "^4.0.0",
    "isomorphic-git": "^1.11.2",
    "lodash": "^4.17.21",
    "monaco-editor": "0.32.1",
    "p-all": "4.0.0",
    "path-browserify": "^1.0.1",
    "preact": "^10.6.5",
    "preact-render-to-string": "^5.1.19",
    "prettier": "^2.5.1",
    "react": "npm:@preact/compat",
    "react-dom": "^17.0.2",
    "textdiff-create": "1.0.7",
    "textdiff-patch": "1.0.6",
    "workbox-cacheable-response": "6.4.2",
    "workbox-cli": "6.4.2",
    "workbox-precaching": "6.4.2",
    "workbox-routing": "6.4.2",
    "workbox-strategies": "6.4.2",
    "workbox-sw": "6.4.2",
    "workbox-window": "6.4.2"
  }
}
