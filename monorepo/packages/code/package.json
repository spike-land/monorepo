{
  "name": "@spike.land/code",
  "version": "0.7.740",
  "description": "spike.land",
  "type": "module",
  "main": "./js/starter.ts",
  "files": [
    "**/*"
  ],
  "exports": {
    "./package.json": "./package.json",
    "./js/starter.ts": "./js/starter.ts",
    "./js/session": "./js/session.ts",
    "./js/esbuildEsm": "./js/esbuildEsm.ts",
    "./js/binary": "./js/binary.ts",
    "./js/importmap.json": "./js/importmap.json",
    "./js/mockedMap.json": "./js/mockedMap.json",
    "./shasums": "./shasums.ts"
  },
  "scripts": {
    "typecheck": "cd monorepo/packages/code && yarn tsc",
    "clean": "rm -rf .tsBuildInfo js/vendor ../../sites/spike.land/public && mkdir ../../sites/spike.land/public js/vendor && yarn favicons && yarn build:framer-motion && yarn build:emotion  && yarn build:preact && yarn wasm",
    "xpostinstall": "yarn importmap",
    "wasm": "cp ../../node_modules/esbuild-wasm/esbuild.wasm ../../sites/spike.land/public/",
    "build:sw": "esbuild --outfile=../sites/spike.land/public/sw.js --platform=browser --bundle --minify ./js/sw.js && yarn sw",
    "start": "cd ../../.. && yarn start",
    "push": "cd ../../.. && yarn push",
    "build:preact": "esbuild --bundle js/preact.ts  --target=esnext --minify --format=esm --platform=browser  --outfile=../../sites/spike.land/public/react.mjs",
    "build:framer-motion": "yarn esbuild --bundle js/motion.tsx  --target=esnext --minify --format=esm  --platform=browser  --define:process.env.NODE_ENV=\\\"production\\\" --external:react --external:@emotion/is-prop-valid  --external:react --outfile=../../sites/spike.land/public/framer-motion.mjs",
    "build:emotion": "yarn esbuild --bundle js/emotion.tsx  --target=esnext --format=esm --external:react --external:react/jsx-runtime --platform=browser --define:process.env.NODE_ENV=\\\"production\\\" --outfile=../../sites/spike.land/public/emotion.mjs",
    "build": "node esbuild-dev.mjs &&  yarn build:sw",
    "favicons": "cp -a -f js/assets/favicons/. ../../sites/spike.land/public/",
    "importmap": "node --experimental-json-modules generate.mjs > ./importmap_NEW.json && mv importmap_NEW.json js/importmap.json",
    "workbox-cli": "workbox-cli",
    "sw": "echo ok",
    "xsw": "yarn workbox injectManifest workbox-config.json"
  },
  "keywords": [
    "monaco",
    "editor",
    "react",
    "typescript",
    "html",
    "vscode"
  ],
  "authors": [
    "Zoltan Erdos <zolika84@gmail.com>"
  ],
  "bugs": {
    "url": "https://github.com/spike-land/monorepo"
  },
  "homepage": "https://spike.land",
  "repository": "https://github.com/spike-land/monorepo",
  "devDependencies": {
    "@types/babel__core": "^7.1.19",
    "@types/lodash": "4.14.182",
    "@types/node": "17.0.31",
    "typescript": "^4.6.4",
    "vite": "2.9.8",
    "vite-plugin-import-maps": "0.1.2"
  },
  "gitHead": "96f8f116ac7d7029d22956f61f6ce225be78cd2c",
  "author": "Zoltan Erdos <zolika84@gmail.com>",
  "license": "BSD-3-Clause",
  "publishConfig": {
    "access": "public"
  },
  "alias": {
    "react": "preact/compat",
    "react-dom": "preact/compat"
  },
  "dependencies": {
    "@aws-crypto/sha256-browser": "^2.0.1",
    "@babel/core": "^7.17.10",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@babel/standalone": "7.17.11",
    "@emotion/cache": "^11.7.1",
    "@emotion/react": "^11.9.0",
    "@emotion/serialize": "^1.0.3",
    "@emotion/sheet": "^1.1.0",
    "@emotion/styled": "^11.8.1",
    "@emotion/utils": "^1.1.0",
    "@mui/material": "^5.6.4",
    "@spike.land/qrious": "^0.7.717",
    "@types/babel__standalone": "^7.1.4",
    "@types/prettier": "^2.6.0",
    "@types/react": "18.0.9",
    "@types/react-dom": "^18.0.3",
    "async-mutex": "0.3.2",
    "comlink": "4.3.1",
    "core-js": "^3.22.4",
    "es-module-shims": "1.5.4",
    "esbuild": "0.14.38",
    "esbuild-plugin-import-map": "2.1.0",
    "esbuild-plugin-jsximportsource": "^0.0.4",
    "esbuild-wasm": "0.14.38",
    "framer-motion": "6.3.3",
    "hoist-non-react-statics": "^3.3.2",
    "immutable": "^4.0.0",
    "lodash": "^4.17.21",
    "monaco-editor": "0.34.0-dev.20220509",
    "p-all": "^4.0.0",
    "path-browserify": "^1.0.1",
    "popmotion": "^11.0.3",
    "preact": "^10.7.1",
    "preact-render-to-string": "^5.2.0",
    "prettier": "^2.6.2",
    "regenerator-runtime": "0.13.9",
    "textdiff-create": "1.0.9",
    "textdiff-patch": "1.0.9",
    "workbox-cacheable-response": "6.5.3",
    "workbox-cli": "6.5.3",
    "workbox-precaching": "6.5.3",
    "workbox-routing": "6.5.3",
    "workbox-strategies": "6.5.3",
    "workbox-sw": "6.5.3",
    "workbox-window": "6.5.3"
  }
}
