    "stream-browserify": "^3.0.0",
    "textdiff-create": "1.0.8",
    "textdiff-patch": "1.0.7",
    "workbox-cacheable-response": "6.5.2",
    "workbox-cli": "6.5.2",
    "workbox-precaching": "6.5.2",
