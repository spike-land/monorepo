{
  "name": "@spike.land/code",
  "version": "0.6.147",
  "description": "spike.land",
  "main": "./js/dist/starter.mjs",
  "files": [
    "**/*"
  ],
  "exports": {
    "./package.json": "./package.json",
    "./js/dist/starter.mjs": "./js/dist/starter.mjs",
    "./js/session.tsx": "./js/session.tsx",
    "./js/importmap.json": "./js/importmap.json"
  },
  "scripts": {
    "clean": "rm -rf .tsBuildInfo js/vendor js/dist && mkdir js/dist js/vendor && deno fmt js && yarn mui:deno && yarn renderToString:deno",
    "xpostinstall": "yarn importmap",
    "build:sw": "yarn esbuild --outfile=./js/sw.js --platform=browser --bundle --minify js/sw.ts && yarn sw",
    "mui:deno": "deno bundle  --unstable  --import-map ./js/importmap.json --no-check ./js/mui.tsx ./js/vendor/mui.mjs ",
    "renderToString:deno": "deno bundle  --unstable  --import-map ./js/importmap.json ./js/renderToString.tsx ./js/vendor/renderToString.mjs --no-check",
    "start": "cd ../../.. && yarn start",
    "push": "cd ../../.. && yarn push",
    "build": "yarn clean && node --experimental-json-modules esbuild-dev.mjs  && yarn build:sw",
    "importmap": "node --experimental-json-modules generate.mjs > ./importmap_NEW.json && mv importmap_NEW.json js/importmap.json",
    "workbox-cli": "workbox-cli",
    "sw": "yarn workbox injectManifest workbox-config.json"
  },
  "keywords": [
    "monaco",
    "editor",
    "react",
    "typescript",
    "html",
    "vscode"
  ],
  "authors": [
    "Zoltan Erdos <zolika84@gmail.com>"
  ],
  "bugs": {
    "url": "https://github.com/spike-land/monorepo"
  },
  "homepage": "https://spike.land",
  "repository": "https://github.com/spike-land/monorepo",
  "devDependencies": {
    "typescript": "4.5.5"
  },
  "gitHead": "d04b8fa0f14eda8ad6efa5a482b75d6bb2958192",
  "author": "Zoltan Erdos <zolika84@gmail.com>",
  "license": "BSD-3-Clause",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@babel/standalone": "^7.17.1",
    "@emotion/cache": "^11.7.1",
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@isomorphic-git/lightning-fs": "isomorphic-git/lightning-fs",
    "@spike.land/qrious": "^0.6.59",
    "@types/babel__standalone": "^7.1.4",
    "@types/isomorphic-git__lightning-fs": "^4.4.2",
    "@types/prettier": "^2.4.3",
    "@types/react": "17.0.39",
    "@types/react-dom": "17.0.11",
    "async-mutex": "^0.3.2",
    "comlink": "4.3.1",
    "es-module-shims": "1.4.4",
    "esbuild-plugin-import-map": "2.1.0",
    "esbuild-wasm": "^0.14.18",
    "framer-motion": "6.2.4",
    "immutable": "^4.0.0",
    "isomorphic-git": "^1.11.2",
    "lodash": "^4.17.21",
    "monaco-editor": "0.32.1",
    "p-all": "4.0.0",
    "prettier": "^2.5.1",
    "react": "next",
    "react-dom": "next",
    "textdiff-create": "1.0.7",
    "textdiff-patch": "1.0.6",
    "workbox-cacheable-response": "6.4.2",
    "workbox-cli": "6.4.2",
    "workbox-precaching": "6.4.2",
    "workbox-routing": "6.4.2",
    "workbox-strategies": "6.4.2",
    "workbox-sw": "6.4.2",
    "workbox-window": "6.4.2"
  }
}
