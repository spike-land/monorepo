{
  "compilerOptions": {
    "allowJs": true,
    "target": "ESNext",
    "module": "ESNext",
    "lib": ["ESNext", "ESNext.Promise","ES2015", "ES2018.Promise"],
    "alwaysStrict": true,
    "isolatedModules": true,
    "strict": true,
    "noEmit": false,
    "outDir": "dist",
    "declaration": true,
    "declarationDir": "dist",
    "preserveConstEnums": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "types": ["@cloudflare/workers-types"]
  }
}
