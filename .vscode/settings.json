{
  "files.exclude": {
    "**/node_modules": true,
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/*.code-search": true,
    "**/dist": true,
    "**/.pnp.*": true
  },
  "search.useIgnoreFiles": true,
  "files.autoSave": "afterDelay",
  "files.autoSaveDelay": 1,
  "git.autorefresh": true,
  "git.autofetch": true,
  "npm.packageManager": "yarn",
  "explorer.expandSingleFolderWorkspaces": true,
  "cSpell.words": [
    "axax",
    "codicon",
    "dockerhub",
    "Erdos",
    "esbuild",
    "esms",
    "ipfs",
    "jsximportsource",
    "Miniflare",
    "onperiodicsync",
    "outfile",
    "pflannery",
    "Saionaro",
    "Sanyi",
    "sess",
    "shadowroot",
    "silvenon",
    "stunprotocol",
    "versionlens"
  ],
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/*/**": true,
    "**/.hg/store/**": true
  },
  "typescript.tsserver.useSyntaxServer": "always",
  "deno.enable": false
}
