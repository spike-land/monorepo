{
  "testing.alwaysRevealTestOnStateChange": false,
  "files.refactoring.autoSave": true,
  "files.exclude": {
    "**/node_modules": true,
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/*.code-search": true,
    "**/dist": true,
    "**/.pnp.*": true,
    "**/.yarn": true
  },
  "search.useIgnoreFiles": true,
  "files.autoSave": "afterDelay",
  "files.autoSaveDelay": 300,
  "git.autorefresh": true,
  "git.autofetch": true,
  "npm.packageManager": "yarn",
  "explorer.expandSingleFolderWorkspaces": true,
  "cSpell.words": [
    "applewebkit",
    "arcanis",
    "axax",
    "bmflb",
    "codicon",
    "devimages",
    "dockerhub",
    "edcore",
    "Erdos",
    "esbuild-wasm",
    "esms",
    "filecache",
    "ipfs",
    "jsximportsource",
    "metafile",
    "Miniflare",
    "onperiodicsync",
    "outfile",
    "pflannery",
    "Saionaro",
    "scrollback",
    "spikeland",
    "stunprotocol",
    "subword",
    "versionlens",
    "zipfs",
    "Zoltan"
  ],
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/*/**": true,
    "**/.hg/store/**": true
  },
  "typescript.tsdk": ".yarn/sdks/typescript/lib",
  "typescript.enablePromptUseWorkspaceTsdk": true,
  "typescript.tsserver.log": "verbose",
  "typescript.tsserver.logToFile": true,
  "typescript.tsserver.logFile": ".yarn/sdks/typescript/tsserver.log",
  "eslint.nodePath": ".yarn/sdks",
  "livePreview.defaultPreviewPath": "/packages/code/src/index.html"
}
