{
  "private": false,
  "name": "@zedvision/spike-land-worker",
  "version": "14.3.3",
  "description": "A template for kick starting a Cloudflare Workers project",
  "main": "dist/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prebuild": "deno fmt src",
